		<script>
			// More info about config & dependencies:
			// - https://github.com/hakimel/reveal.js#configuration
			// - https://github.com/hakimel/reveal.js#dependencies
			Reveal.initialize({

          controls: false,
          progress: false,
          center: true,
          hash: true,
          transition: "fade",
          transitionSpeed: "normal",
          viewDistance: 5,

          keyboard: {
              84: 'next'
          },

            dependencies: [
            { src: 'plugin/markdown/marked.js' },
            { src: 'plugin/markdown/markdown.js' },
            { src: 'plugin/notes/notes.js', async: true },
            { src: 'plugin/highlight/highlight.js', async: true }
            ]

			});
     Reveal.addKeyBinding( { keyCode: 88, key: 'X', description: 'Normal speed' }, function() {
         Reveal.configure( { transitionSpeed: 'normal' } );
     })
     Reveal.addKeyBinding( { keyCode: 90, key: 'Z', description: 'Slow speed' }, function() {
         Reveal.configure( { transitionSpeed: 'slow' } );
     })
     Reveal.addKeyBinding( { keyCode: 67, key: 'C', description: 'Fast speed' }, function() {
         Reveal.configure( { transitionSpeed: 'fast' } );
     })
		</script>
